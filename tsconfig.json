{
  "extends": "./node_modules/expo/tsconfig.base",
  "compilerOptions": {
    "module": "es2022",
    "strict": true,
    "jsx": "react-native",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.json"
  ]
}
